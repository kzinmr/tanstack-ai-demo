# LLM API Key (choose one)
OPENAI_API_KEY=your-openai-api-key
# GEMINI_API_KEY=your-gemini-api-key

# Model selection (optional, defaults to openai:gpt-4o-mini)
# LLM_MODEL=openai:gpt-4o-mini
# LLM_MODEL=google-gla:gemini-2.5-flash

# Database connection
DATABASE_URL=postgresql://postgres:postgres@localhost:54320/pydantic_ai_sql_gen

# CORS origins (comma-separated list)
# CORS_ORIGINS=["http://localhost:5173","http://127.0.0.1:5173"]

# Data store TTL in minutes (optional, defaults to 30)
# CSV_DATA_TTL_MINUTES=30

# SQL max limit (optional, defaults to 1000)
# SQL_MAX_LIMIT=1000
