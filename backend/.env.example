# LLM API Key (choose one)
OPENAI_API_KEY=your-openai-api-key
# GEMINI_API_KEY=your-gemini-api-key

# Model selection (optional, defaults to openai:gpt-5-mini)
# LLM_MODEL=openai:gpt-5-mini
# LLM_MODEL=google-gla:gemini-2.5-flash

# Database connection
DATABASE_URL=postgresql://postgres:postgres@localhost:54320/pydantic_ai_sql_gen

# CORS origins (comma-separated list)
# CORS_ORIGINS=["http://localhost:5173","http://127.0.0.1:5173"]

# Data store TTL in minutes (optional, defaults to 30)
# CSV_DATA_TTL_MINUTES=30

# Run store backend (memory, redis, s3)
# RUN_STORE_BACKEND=memory

# Artifact store backend (memory, s3)
# ARTIFACT_STORE_BACKEND=memory

# Redis URL (for custom run store adapters)
# REDIS_URL=redis://localhost:6379/0

# S3 artifact store configuration (only when ARTIFACT_STORE_BACKEND=s3)
# S3_BUCKET=your-bucket
# S3_PREFIX=tanstack-ai-demo
# S3_REGION=us-east-1
# S3_ENDPOINT_URL=https://s3.amazonaws.com
# S3_USE_PATH_STYLE=false
# S3_SIGNED_URL_EXPIRES_IN=900
# S3_PREVIEW_ROWS=200

# SQL max limit (optional, defaults to 1000)
# SQL_MAX_LIMIT=1000
